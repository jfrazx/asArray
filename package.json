{
  "name": "@jfrazx/asarray",
  "version": "0.0.0-development",
  "description": "Ensures returned value is always an Array",
  "main": "dist/index.js",
  "scripts": {
    "commit": "git-cz",
    "prebuild": "rimraf dist",
    "build": "tsc",
    "prepublish": "npm run build",
    "coverage:check": "nyc check-coverage",
    "coverage:report": "nyc report --reporter=text-lcov > coverage.lcov",
    "coverage:post": "codecov",
    "precoverage:test": "rimraf coverage",
    "coverage:test": "nyc mocha",
    "precommit": "npm run coverage:test && npm run coverage:check",
    "prepush": "npm run coverage:test",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post",
    "test": "mocha"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jfrazx/asArray.git"
  },
  "keywords": [
    "array",
    "arguments",
    "convert",
    "object",
    "typed array",
    "keys",
    "typescript"
  ],
  "author": "jfrazx",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jfrazx/asArray/issues"
  },
  "homepage": "https://github.com/jfrazx/asArray#readme",
  "devDependencies": {
    "@istanbuljs/nyc-config-typescript": "^1.0.1",
    "@semantic-release/commit-analyzer": "^8.0.1",
    "@semantic-release/release-notes-generator": "^9.0.2",
    "@types/chai": "^4.2.15",
    "@types/mocha": "^8.2.2",
    "chai": "^4.3.4",
    "codecov": "^3.8.1",
    "commitizen": "^4.2.3",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^7.23.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "husky": "^6.0.0",
    "mocha": "^8.3.2",
    "nyc": "^15.1.0",
    "prettier": "^2.2.1",
    "rimraf": "^3.0.2",
    "semantic-release": "^19.0.3",
    "source-map-support": "^0.5.19",
    "travis-deploy-once": "^5.0.11",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.3"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    }
  },
  "mocha": {
    "recursive": false,
    "spec": "test/*.spec.ts",
    "reporter": "spec",
    "require": [
      "ts-node/register/transpile-only",
      "source-map-support/register"
    ]
  }
}
